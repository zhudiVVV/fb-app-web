.mp-search-box {
  position: absolute;
  left: 0;
  top: 30rpx;
  z-index: 9999;
  width: 100%;
  padding: 0 80rpx;
}
.mp-search-box .ser-input {
  flex: 1;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  font-size: 28rpx;
  color: #606266;
  border-radius: 20px;
  background: rgba(255, 255, 255, 0.6);
}
page .cate-section {
  position: relative;
  z-index: 5;
  border-radius: 16rpx 16rpx 0 0;
  margin-top: -20rpx;
}
page .carousel-section {
  padding: 0;
}
page .carousel-section .titleNview-placing {
