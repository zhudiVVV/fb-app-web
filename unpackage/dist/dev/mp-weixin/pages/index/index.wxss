/* 头部 轮播图 */
.carousel-section {
  position: relative;
  padding-top: 10px;
}
.carousel-section .titleNview-placing {
  height: 25px;
  padding-top: 44px;
  box-sizing: content-box;
}
.carousel-section .titleNview-background {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 426rpx;
  transition: .4s;
}
.carousel {
  width: 100%;
  height: 350rpx;
}
.carousel .carousel-item {
  width: 100%;
  height: 100%;
  padding: 0 28rpx;
  overflow: hidden;
}
.carousel image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.swiper-dots {
  display: flex;
  position: absolute;
  left: 60rpx;
  bottom: 15rpx;
  width: 72rpx;
  height: 36rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.swiper-dots .num {
  width: 36rpx;
  height: 36rpx;
  border-radius: 50px;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 36rpx;
}
.swiper-dots .sign {
  position: absolute;
  top: 0;
  left: 50%;
  line-height: 36rpx;
  font-size: 12rpx;
  color: #fff;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
/* 分类 */
.cate-section {
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: wrap;
  padding: 30rpx 22rpx;
  background: #fff;
  /* 原图标颜色太深,不想改图了,所以加了透明度 */
}
.cate-section .cate-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 26rpx;
  color: #303133;
}
.cate-section image {
  width: 88rpx;
  height: 88rpx;
  margin-bottom: 14rpx;
  border-radius: 50%;
  opacity: .7;
}
.ad-1 {
  width: 100%;
  height: 210rpx;
  padding: 10rpx 0;
  background: #fff;
}
.ad-1 image {
  width: 100%;
  height: 100%;
}
/* 秒杀专区 */
.seckill-section {
  padding: 4rpx 30rpx 24rpx;
  background: #fff;
}
.seckill-section .s-header {
  display: flex;
  align-items: center;
  height: 92rpx;
  line-height: 1;
}
.seckill-section .s-header .s-img {
  width: 140rpx;
  height: 30rpx;
}
.seckill-section .s-header .tip {
  font-size: 28rpx;
  color: #909399;
  margin: 0 20rpx 0 40rpx;
}
.seckill-section .s-header .timer {
  display: inline-block;
  width: 40rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  margin-right: 14rpx;
  font-size: 26rpx;
  color: #fff;
  border-radius: 2px;
  background: rgba(0, 0, 0, 0.8);
}
.seckill-section .s-header .icon-you {
  font-size: 32rpx;
  color: #909399;
  flex: 1;
  text-align: right;
}
.seckill-section .floor-list {
  white-space: nowrap;
}
.seckill-section .scoll-wrapper {
  display: flex;
  align-items: flex-start;
}
.seckill-section .floor-item {
  width: 300rpx;
  margin-right: 20rpx;
  font-size: 26rpx;
  color: #303133;
  line-height: 1.8;
}
.seckill-section .floor-item image {
  width: 300rpx;
  height: 300rpx;
  border-radius: 6rpx;
}
.seckill-section .floor-item .price {
  color: #fa436a;
}
.seckill-section .title2 {
  font-size: 24rpx;
  color: #909399;
  line-height: 40rpx;
}
.f-header {
  display: flex;
  align-items: center;
  height: 140rpx;
  padding: 6rpx 30rpx 8rpx;
  background: #fff;
}
.f-header image {
  flex-shrink: 0;
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}
.f-header .tit-box {
  flex: 1;
  display: flex;
  flex-direction: column;
}
.f-header .tit {
  font-size: 34rpx;
  color: #font-color-dark;
  line-height: 1.3;
}
.f-header .tit2 {
  font-size: 24rpx;
  color: #909399;
}
.f-header .icon-you {
  font-size: 34rpx;
  color: #909399;
}
.f-header .timer {
  display: inline-block;
  width: 40rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  margin-right: 14rpx;
  font-size: 26rpx;
  color: #fff;
  border-radius: 2px;
  background: rgba(0, 0, 0, 0.8);
}
/* 分类推荐楼层 */
.hot-floor {
  width: 100%;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.hot-floor .floor-img-box {
  width: 100%;
  height: 320rpx;
  position: relative;
}
.hot-floor .floor-img-box:after {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(rgba(255, 255, 255, 0.06) 30%, #f8f8f8);
}
.hot-floor .floor-img {
  width: 100%;
  height: 100%;
}
.hot-floor .floor-list {
  white-space: nowrap;
  padding: 20rpx;
  padding-right: 50rpx;
  border-radius: 6rpx;
  margin-top: -140rpx;
  margin-left: 30rpx;
  background: #fff;
  box-shadow: 1px 1px 5px rgba(0, 0, 0, 0.2);
  position: relative;
  z-index: 1;
}
.hot-floor .scoll-wrapper {
  display: flex;
  align-items: flex-start;
}
.hot-floor .floor-item {
  width: 180rpx;
  margin-right: 20rpx;
  font-size: 26rpx;
  color: #303133;
  line-height: 1.8;
}
.hot-floor .floor-item image {
  width: 180rpx;
  height: 180rpx;
  border-radius: 6rpx;
}
.hot-floor .floor-item .price {
  color: #fa436a;
}
.hot-floor .more {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  flex-shrink: 0;
  width: 180rpx;
  height: 180rpx;
  border-radius: 6rpx;
  background: #f3f3f3;
  font-size: 28rpx;
  color: #909399;
}
.hot-floor .more text:first-child {
  margin-bottom: 4rpx;
}
/* 猜你喜欢 */
.guess-section {
  display: flex;
  flex-wrap: wrap;
  padding: 0 30rpx;
  background: #fff;
}
.guess-section .guess-item {
  display: flex;
  flex-direction: column;
  width: 48%;
  padding-bottom: 40rpx;
}
.guess-section .guess-item:nth-child(2n+1) {
  margin-right: 4%;
}
.guess-section .image-wrapper {
  width: 100%;
  height: 330rpx;
  border-radius: 3px;
  overflow: hidden;
}
.guess-section .image-wrapper image {
  width: 100%;
  height: 100%;
  opacity: 1;
}
.guess-section .image-wrapper-brand {
  width: 100%;
  height: 150rpx;
  border-radius: 3px;
  overflow: hidden;
}
.guess-section .image-wrapper-brand image {
  width: 100%;
  height: 100%;
  opacity: 1;
}
.guess-section .title {
  font-size: 32rpx;
  color: #303133;
  line-height: 80rpx;
}
.guess-section .title2 {
  font-size: 24rpx;
  color: #909399;
  line-height: 40rpx;
}
.guess-section .price {
  font-size: 32rpx;
  color: #fa436a;
  line-height: 1;
}
.hot-section {
  display: flex;
  flex-wrap: wrap;
  padding: 0 30rpx;
  background: #fff;
}
.hot-section .guess-item {
  display: flex;
  flex-direction: row;
  width: 100%;
  padding-bottom: 40rpx;
}
.hot-section .image-wrapper {
  width: 30%;
  height: 250rpx;
  border-radius: 3px;
  overflow: hidden;
}
.hot-section .image-wrapper image {
  width: 100%;
  height: 100%;
  opacity: 1;
}
.hot-section .title {
  font-size: 32rpx;
  color: #303133;
  line-height: 80rpx;
}
.hot-section .title2 {
  font-size: 24rpx;
  color: #909399;
  line-height: 40rpx;
  height: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: block;
}
.hot-section .price {
  font-size: 32rpx;
  color: #fa436a;
  line-height: 80rpx;
}
.hot-section .txt {
  width: 70%;
  display: flex;
  flex-direction: column;
  padding-left: 40rpx;
}

